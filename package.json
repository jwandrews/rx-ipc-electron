  "devDependencies": {
    "@types/chai": "^4.2.4",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.12",
    "chai": "^4.2.0",
    "electron": "^7.1.1",
    "electron-mocha": "^8.1.2",
    "mocha-typescript": "^1.1.17",
    "rimraf": "^3.0.0",
